<?xml version="1.0" encoding="utf-8" ?>
<rss version="2.0">
  <channel>
    <title>$title$</title>
    <description>$description$</description>
    <link>https://austinplunkett.com/</link>
    <!-- atom:link href="https://austinplunkett.com/rss.xml" rel="self" type="application/rss+xml" / -->
    <lastBuildDate>$updated$</lastBuildDate>

    $for(references)$
    <item>
      <title>$references.title$</title>
      <description>$references.abstract$</description>
      <link>$references.url$</link>
      <pubDate>$references.issued$</pubDate>
      <!-- guid>$references.url$@$references.issued$</guid -->
    </item>
    $endfor$
  </channel>
</rss>
